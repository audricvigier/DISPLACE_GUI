#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

export QT_SELECT := opt-qt56


# This has to be exported to make some magic below work.
export DH_OPTIONS


%:
	dh $@ --parallel

override_dh_auto_configure:
	dh_auto_configure -- -DCMAKE_PREFIX_PATH=/opt/qt56/lib/cmake

override_dh_shlibdeps:
	dh_shlibdeps -l/opt/qt56/lib

